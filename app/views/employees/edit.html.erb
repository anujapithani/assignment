 <div id="wrapper">
    <%= render "mains/navbar" %>
    <%= render "mains/sidebar" %>
      <div class="content">
        <div class="container-fluid">
          <div class="container">
          
              <h2>edit page</h2>
              <%= form_with(model: @employee, local: true) do |form| %>
              <h1>Employee registration</h1>

          <div class="form-group p-2">
              <strong><%= form.label :name %></strong><br>
              <%= form.text_field :name, class:'form-control' %>
           </div>

           <div class="form-group p-2">
              <strong><%= form.label :email %></strong><br>
              <%= form.email_field :email, class: 'form-control' %>
           </div>

           <div class="form-group p-2">
              <strong><%= form.label :empid %></strong><br>
              <%= form.text_field :empid, class: 'form-control' %>
           </div>

           <div class="form-group p-2">
              <%= form.submit :register, class: 'form-control btn-light' %>
           </div>

        <% end %>
      </div>
    </div>
  </div>
</div>